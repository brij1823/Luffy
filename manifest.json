{
    "manifest_version" :2,
    "name" :"Equality",
    "version" :"1.0",
    "description" : "Equality for all",
    "icons":{
        "128":"icon128.png",
        "64":"icon64.png",
        "16":"icon16.png"
    },
    "browser_action": {
        "default_popup" : "popout.html"
    },
    "content_scripts":[
        {
            "matches" : ["*://*/*"],
            "all_frames" : true,
            "js" : ["content_script.js"],
            "run_at":"document_end"
        }
    ]

   }